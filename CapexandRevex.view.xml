<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
    controllerName="infraproject.controller.Infra">

    <Page id="IntegratedPaymentManagementPortalPage" title="INTEGRATED PAYMENT MANAGEMENT PORTAL" showNavButton="true" navButtonPress="onNavBack">
        <customData>
            <core:CustomData key="sap-ui-custom-style" value="css/IntegratedPaymentManagementPortal.css" />
        </customData>

        <uxap:ObjectPageLayout id="ObjectPageLayoutPortal" showTitleInHeaderContent="true" upperCaseAnchorBar="false" showHeaderContent="true" showFooter="true">
            <uxap:headerTitle>
                <uxap:ObjectPageDynamicHeaderTitle id="ObjectPageDynamicHeaderTitle" class="customHeaderClass">
                    <uxap:actions>
                        <HBox class="customhead">
                            <HBox>
                                <Text id="inputRequestIDPortal" class="textfont" text="Request Number : {PortalModel>/reqNo}" width="100%" />
                            </HBox>
                            <HBox id="ActionButtons" class="customButtonContainer">
                                <Button id="SaveBtn" text="Save" type="Default" class="customButtonSave" press="onSaveForm" />
                                <Button id="SubmitBtn" text="Submit" type="Emphasized" class="customButtonSubmit" press="onSubmitForm" />
                            </HBox>
                        </HBox>
                    </uxap:actions>
                </uxap:ObjectPageDynamicHeaderTitle>
            </uxap:headerTitle>

            <uxap:headerContent>
                <VBox id="FormContainer" justifyContent="Start">
                    <form:SimpleForm
                        id="PortalForm"
                        editable="true"
                        layout="ColumnLayout"
                        labelSpanXL="12"
                        labelSpanL="12"
                        labelSpanM="12"
                        columnsXL="4"
                        columnsL="4"
                        columnsM="1"
                        adjustLabelSpan="false"
                        class="sapUiSmallMarginBeginEnd customFormClass">

<form:content>

    <!-- Row 1 -->
    <Label text="Requester Token/Name" required="true" />
    <Input value="50002151 - PRIYATHAM TELAPROLU" editable="false" width="100%" />

    <Label text="Department" required="true" />
    <Input value="TEMP" editable="false" width="100%" />

    <Label text="Date" />
    <DatePicker value="2023-08-02" displayFormat="dd-MM-yyyy" valueFormat="yyyy-MM-dd" editable="false" width="100%" />

    <Label text="Request Number" />
    <Input value="{PortalModel>/reqNo}" editable="false" width="100%" />

    <!-- Row 2 -->
    <Label text="Payment Option" required="true" />
    <ComboBox selectedKey="{PortalModel>/paymentOption}" width="100%" placeholder="Select Payment Option">
        <!-- <core:Item text="Select Payment Option" key="default" /> -->
        <core:Item text="Advance Payment" key="Advance Payment" />
        <core:Item text="Regular Payment" key="Regular Payment" />
    </ComboBox>

    <Label text="Payment Type" required="true" />
    <ComboBox selectedKey="{PortalModel>/paymentType}" width="100%" placeholder="Select Payment Type">
        <!-- <core:Item text="Select Payment Type" key="default" /> -->
        <core:Item text="Capex" key="Capex" />
        <core:Item text="Revex" key="Revex" />
    </ComboBox>

    <Label text="PO/Non PO" required="true" />
    <ComboBox selectedKey="{PortalModel>/poNonPO}" width="100%" placeholder="Select Payment Option">
        <!-- <core:Item text="Select Payment Option" key="default" /> -->
        <core:Item text="PO" key="po" />
        <core:Item text="Non-PO" key="nonpo" />
    </ComboBox>

    <!-- Row 3 -->
    <Label text="Accounting Document Number" />
    <Input value="{PortalModel>/accountingDocNumber}" placeholder="Enter Accounting Document Number" width="100%" />

    <Label text="Payment Date" required="true" />
    <DatePicker value="{PortalModel>/paymentDate}" displayFormat="dd-MM-yyyy" valueFormat="yyyy-MM-dd" placeholder="Select Payment Date" width="100%" />

    <Label text="PO" />
    <Input value="{PortalModel>/po}" placeholder="Enter PO" width="100%" />

    <Label text="Vendor Code" />
    <Input value="{PortalModel>/vendorCode}" placeholder="Enter Vendor Code" width="100%" />

    <!-- Row 4 -->
    <Label text="Cost Center" required="true" />
    <Input value="{PortalModel>/costCenter}" placeholder="Enter Cost Center" width="100%" />

    <Label text="WBS" required="true" />
    <Input value="{PortalModel>/wbs}" placeholder="Enter WBS" width="100%" />

    <Label text="Total Amount" />
    <Input value="{PortalModel>/totalAmount}" type="Number" placeholder="Enter Total Amount" width="100%" />

    <Label text="Vendor Name" required="true" />
    <Input value="{PortalModel>/vendorName}" placeholder="Enter Vendor Name" width="100%" />

    <!-- Row 5 -->
    <Label text="Invoice Number" required="true" />
    <Input value="{PortalModel>/invoiceNumber}" placeholder="Enter Invoice/Proforma Number" width="100%" />

    <Label text="Base Amount" required="true" />
    <Input value="{PortalModel>/baseAmount}" type="Number" placeholder="Enter Base Amount" width="100%" />

    <Label text="GST" />
    <Input value="{PortalModel>/gst}" type="Number" placeholder="Enter GST" width="100%" />

    <Label text="TDS" />
    <Input value="{PortalModel>/tds}" type="Number" placeholder="Enter TDS" width="100%" />

    <!-- Row 6 -->
    <Label text="Buyer Requester" required="true" />
    <ComboBox selectedKey="{PortalModel>/buyerRequester}" width="100%" placeholder="Select Requestor">
        <!-- <core:Item text="Select Requestor" key="default" /> -->
        <core:Item text="Requestor 1" key="req1" />
        <core:Item text="Requestor 2" key="req2" />
    </ComboBox>

    <Label text="Buyer HOD" required="true" />
    <ComboBox selectedKey="{PortalModel>/buyerHOD}" width="100%" placeholder="Select HOD">
        <!-- <core:Item text="Select HOD" key="default" /> -->
        <core:Item text="HOD 1" key="hod1" />
        <core:Item text="HOD 2" key="hod2" />
    </ComboBox>

    <Label text="Payment Terms" />
    <ComboBox selectedKey="{PortalModel>/paymentTerms}" width="100%" placeholder="Select Payment Terms">
        <!-- <core:Item text="Select Payment Terms" key="default" /> -->
        <core:Item text="Term 1" key="term1" />
        <core:Item text="Term 2" key="term2" />
    </ComboBox>

    <!-- Final Row: Remarks -->
    <!-- <Label text="Remarks" />
    <TextArea value="{PortalModel>/remarks}" placeholder="Enter Remarks" width="100%" /> -->

</form:content>
                    </form:SimpleForm>
                </VBox>
            </uxap:headerContent>
        </uxap:ObjectPageLayout>
    </Page>
</mvc:View>



